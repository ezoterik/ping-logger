{
    "name": "Ping Logger",
    "description": "Ping Logger Application",
    "keywords": [
        "ping",
        "lan",
        "network",
        "utility",
        "monitoring",
        "dashboard"
    ],
    "homepage": "https://bitbucket.org/ezoterik/ping_logger",
    "type": "project",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Oleg Balykin",
            "email": "ezoterik.h@gmail.com",
            "homepage": "http://ezoterik.name",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://bitbucket.org/ezoterik/ping_logger"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "2.0.7",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-icons": "1.4.*",
        "omnilight/yii2-momentjs": "1.0.*",
        "baibaratsky/yii2-rollbar": "1.3.*",
        "ezoterik/yii2-rollbar-js": "1.0.*",
        "bower-asset/react": "0.13.*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "suggest": {
        "codeception/codeception": "Codeception, 1.8.*@dev is currently works well with Yii.",
        "codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
        "codeception/verify": "BDD Assertions for PHPUnit and Codeception"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web-local.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}